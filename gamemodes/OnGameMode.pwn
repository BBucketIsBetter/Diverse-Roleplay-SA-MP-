public OnGameModeInit()
{
	//==============================//
	TryMe();
	//==============================//
    sqlconnect();
	print("Connected to MySQL werd up.");
	//==============================//
	ManualVehicleEngineAndLights();
	//=======Add PData Fields=======//
	AddPDataField("Mapper",0,1);
	AddPDataField("9mmSkill",0,3);
	AddPDataField("uziSkill",0,3);
	AddPDataField("sawnoffSkill",0,3);
	//==============================//
	new tmphour, tmpminute, tmpsecond, str[128],
	year, month, day;
	//==============================//
	SetGameModeText(CFG_MODE);
	format(str, sizeof(str),"mapname %s", CFG_MAPNAME);
	SendRconCommand(str);
	format(str, sizeof(str),"weburl %s", CFG_WEBSITE);
	SendRconCommand(str);
	DisableInteriorEnterExits();
	ShowPlayerMarkers(0);
	ShowNameTags(1);
	SetNameTagDrawDistance(15.0);
	EnableStuntBonusForAll(0);
	Streamer_SetCellDistance(1000.0);
	//Streamer_MaxItems(STREAMER_TYPE_OBJECT, 2000);
	Streamer_VisibleItems(STREAMER_TYPE_OBJECT, 2000);
	//==============================//
	LoadMaps();
	LoadInts();
	LoadFactions();
	LoadHouses();
	LoadBizzes();
	//==============================//
	mysql_tquery(handlesql, "SELECT * FROM `mapfurn`", "LoadMap");
	mysql_tquery(handlesql, "SELECT * FROM `gates`", "thread_LoadGates");
	mysql_tquery(handlesql, "SELECT * FROM `payphones`", "thread_LoadPhones");
	//==============================//
	new weather = random(15)+1;
	switch(weather)
	{
	    case 0,1: Weather=1;
		case 2: Weather=2;
		case 3: Weather=3;
		case 4: Weather=4;
		case 5: Weather=5;
		case 6: Weather=6;
		case 7,8,9: Weather=7;
		case 10: Weather=10;
		case 11: Weather=11;
		case 12: Weather=12;
		case 13: Weather=13;
		case 14: Weather=18;
		case 15: Weather=9;
	}
    SetWeatherEx(Weather);
	//==============================//
	gettime(tmphour, tmpminute, tmpsecond), getdate(year, month, day);
	FixHour(tmphour);
	SetWorldTime(tmphour);
	ghour = tmphour, gday=day;
	//==============================//
	AddPlayerClass(25, 21642.7285,-2240.5591,13.4945,180.0272, 0, 0, 0, 0, 0, 0);
